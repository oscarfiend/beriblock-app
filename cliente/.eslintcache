eight: 3vw;\r\n  padding: 0.3vw;\r\n  width: 98%;\r\n  font-family: Anybodythink;\r\n`;\r\n\r\nconst Select = styled.select`\r\n  font-size: calc(12px + 10 * ((100vw - 320px) / 680));\r\n  height: 4vw;\r\n  padding: 0.3vw;\r\n  width: 100%;\r\n  font-family: Anybodythink;\r\n`;\r\n\r\nconst Row = styled.div`\r\n  display: flex;\r\n  flex-wrap: nowrap;\r\n\r\n  @media (max-width: 540px) {\r\n    flex-direction: column;\r\n  }\r\n`;\r\n\r\nconst Col = styled.div`\r\n  flex: 50%;\r\n  margin-right: 2vw;\r\n  margin-top: 2vw;\r\n`;\r\n\r\nconst FormUser = ({ user, setShowForm, setuserSelected,setUsers,users,setPage,totalPages }) => {\r\n  const [newUser, setNewUser] = useState({\r\n    name: \"\",\r\n    email: \"\",\r\n    gender: \"Male\",\r\n    status: \"Active\",\r\n  });\r\n\r\n  const myRef = useRef()\r\n\r\n  useEffect(() => {\r\n    if (user !== null) {\r\n      setNewUser(user);\r\n    }\r\n    myRef.current.scrollIntoView();\r\n  }, [user]);\r\n\r\n  const { name, email, gender, status } = newUser;\r\n\r\n  const handleChange = (e) => {\r\n\r\n    setNewUser({\r\n      ...newUser,\r\n      [e.target.name]: e.target.value,\r\n    });\r\n  };\r\n\r\n  const handleCancel = () => {\r\n    setShowForm(false);\r\n    setuserSelected(null);\r\n  };\r\n\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n    //validate inputs\r\n    if (name.trim() === \"\" || email.trim() === \"\") {\r\n      Swal.fire(\"All fields are required\");\r\n      return\r\n    }\r\n\r\n    if (!isValid(email)) {\r\n      Swal.fire(\"The email is not valid\");\r\n      return\r\n    }\r\n\r\n    //if all is ok\r\n    //save date\r\n    if (user) {\r\n      updateUser(user.id,newUser,setUsers,users);\r\n    } else {\r\n      addUser(newUser,setPage,totalPages);\r\n    }\r\n\r\n    //reset form\r\n    handleCancel()\r\n  };\r\n\r\n  const isValid = (email) => {\r\n    if (\r\n      /^(([^<>()[\\]\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@(([^<>()[\\]\\.,;:\\