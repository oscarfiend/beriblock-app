ges, setTotalPages] = useState(1);\r\n  const [userSelected, setuserSelected] = useState(null);\r\n  const [showForm, setShowForm] = useState(false);\r\n\r\n  const refUsers = useRef();\r\n\r\n  useEffect(() => {\r\n      getUsers(setLoading,setUsers,setTotalPages,page);\r\n      if(users.length!==0){\r\n          refUsers.current.scrollIntoView();\r\n      }\r\n  }, [page]);  \r\n\r\n  return (\r\n    <Container>\r\n    {\r\n        !showForm?\r\n        <Button onClick={()=>setShowForm(!showForm)}>New User</Button>\r\n        :\r\n        <FormUser \r\n        user={userSelected} \r\n        setUsers={setUsers} \r\n        users={users}  \r\n        setShowForm={setShowForm} \r\n        setuserSelected={setuserSelected}\r\n        setPage={setPage}\r\n        totalPages={totalPages}\r\n        />\r\n    }\r\n      {loading && <Spinner />}\r\n      {users.length !== 0 && (\r\n        <div ref={refUsers}>\r\n          <Table>\r\n            <thead>\r\n              <tr>\r\n                <Title>#</Title>\r\n                <Title>Name</Title>\r\n                <Title>Email</Title>\r\n                <Title>Gender</Title>\r\n                <Title>Status</Title>\r\n                <Title>Actions</Title>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              {users.map((user) => (\r\n                <User \r\n                user={user} \r\n                key={user.id} \r\n                setUser={setUsers} \r\n                users={users} \r\n                setuserSelected={setuserSelected}\r\n                setShowForm={setShowForm}\r\n                />\r\n              ))}\r\n            </tbody>\r\n          </Table>\r\n        <Pagination setPage={setPage} totalPages={totalPages}/>\r\n        </div>\r\n      )}\r\n    </Container>\r\n  );\r\n};\r\n\r\nexport default ListUsers;\r\n","C:\\Users\\oscar tirado\\Documents\\proyectos\\React\\Proyectos_propios\\mg-group-project\\src\\components\\users\\User.js",[],"C:\\Users\\oscar